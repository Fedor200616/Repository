#include <iostream>

int main() {
	int* ptr = new int;
	*ptr = 5;
	delete ptr;
	if (ptr) {
		std::cout << "Pointer is not null after delete." << std::endl;
	}
	else {
		std::cout << "Pointer is null after delete." << std::endl;
        }
	if (1) {
          ptr = 2;
        }
     
	  
	return 0;
}
